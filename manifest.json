{
    "manifest_version": 3,
    "name": "UwUWWW",
    "description": "An extension that UwUifies all text on sites.",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "assets/uwu.png"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "storage",
        "webNavigation",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
